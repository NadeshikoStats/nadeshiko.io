  playerData = JSON.parse(decodeURIComponent("<%- encodeURIComponent(JSON.stringify(playerData)) %>"));

  let playerAchievements = playerData["achievements"] || {};

  const imageUrls = [
    `/img/games/network/lobby.webp`,
    `/img/games/arcade/lobby.webp`,
